<!DOCTYPE html>
<html lang="en" data-ng-app="@ViewBag.InitModule">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta charset="utf-8">
    <title>@ViewBag.Title - Computer Customize and Rating</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="keywords" content="">

    <!-- HTML5 Support for IE -->
    <!--[if IE 9]>
    <script src="Scripts/html5shim.js"></script>
    <![endif]-->

    <link href='http://fonts.googleapis.com/css?family=Dosis:200,400,600' rel='stylesheet' type='text/css'>
    
    <link href="~/Content/Template/style/bootstrap.css" rel="stylesheet">
    <link rel="stylesheet" href="~/Content/Template/style/font-awesome.css">
    <!--[if IE 7]>
    <link rel="stylesheet" href="~/Content/Template/style/font-awesome-ie7.css">
    <![endif]-->

    <link href="~/Content/themes/base/jquery-ui.css" rel="stylesheet" />
    <link href="~/Content/Template/style/ddlevelsmenu-base.css" rel="stylesheet">
    <link href="~/Content/Template/style/ddlevelsmenu-topbar.css" rel="stylesheet">
    <link href="~/Content/Template/style/style.css" rel="stylesheet">
    <link href="~/Content/Template/style/blue.css" rel="stylesheet">
    <link href="~/Content/Template/style/bootstrap-responsive.css" rel="stylesheet">
    <link href="~/Content/Template/style/flexslider.css" rel="stylesheet">
    <link href="~/Content/Template/style/prettyPhoto.css" rel="stylesheet">
    <link href="~/Content/Style.css" rel="stylesheet" />
    <link href="~/Content/PagedList.css" rel="stylesheet" />
    <link rel="shortcut icon" href="~/Content/Template/img/favicon/favicon.png">
</head>

<body>

    <div class="sharepanel">
        <div class="container">
            <div class="row">
                <div class="span8">
                    <div class="padd spara">
                        <p>Share good ones with your friends!</p>
                    </div>
                </div>
                <div class="span4">
                    <div class="padd">
                        <div class="social">
                            <a href="#">
                                <img src="~/Content/Template/img/facebook.png" alt="" width="40" /></a>
                            <a href="#">
                                <img src="~/Content/Template/img/twitter.png" alt="" width="40" /></a>
                            <a href="#">
                                <img src="~/Content/Template/img/linkedin.png" alt="" width="40" /></a>
                            <a href="#">
                                <img src="~/Content/Template/img/googleplus.png" alt="" width="40" /></a>
                            <a href="#">
                                <img src="~/Content/Template/img/rss.png" alt="" width="40" /></a>
                        </div>
                    </div>
                </div>
            </div>
            <div class="clearfix"></div>
        </div>
    </div>


    <div class="fixedbar">
        <div class="container">
            <div class="row">
                <div class="span12">
                    <div class="logo">
                        <div class="pull-left">
                            <h1><a href="~/Home/"><i class="icon-camera-retro"></i>PC Customize</a></h1>
                        </div>
                        <div class="navi">
                            <div class="pull-left">
                                
                                <div id="ddtopmenubar" class="mattblackmenu">
                                    <ul>
                                        <li>@Html.ActionLink("Home", "Index", "Home")
                                        </li>
                                        <li>@Html.ActionLink("Computer", "Index", "Computer")
                                        </li>
                                        <li>@Html.ActionLink("About", "About", "Home")
                                        </li>
                                        @Html.Partial("_LoginPartial")
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="share">
                        <div class="pull-right">
                            <p><a href="#" class="open"><span class="hidden-phone">Share it</span> <i class="icon-share"></i></a></p>
                        </div>
                    </div>
                </div>
                <div class="clearfix"></div>
            </div>
        </div>
    </div>


    <div class="main">
        <div class="container">
            <div class="row">
                <div class="span12 hidden-desktop">
                    <div class="padd">
                        <div class="navis"></div>
                    </div>
                </div>
                <div class="span12">
                    <div class="whity content">
                        <div class="posts">
                            <div class="entry">
                                <div class="padd">
                                    <h2>
                                        @ViewBag.PadTitle
                                    </h2>
                                </div>

                                <div class="row">
                                    <div class="span12">
                                        <div class="padd">
                                            <div class="co-form main-content">
                                                @RenderBody()

                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>


    <div class="foot">
        <div class="container">
            <div class="row">
                <div class="span4">
                    <div class="padd">
                        <div class="widget">
                            <h3>Idea of the Site</h3>
                            <blockquote>
                                <p>The website is for people who wants to buy either desktop or laptop but wants to customize by themselves. People are able to customize computers by their own, rate and share the customized ones of others.</p>
                                <small>Zack Cao</small>
                            </blockquote>
                        </div>
                    </div>
                </div>
                <div class="span4">
                    <div class="padd">
                        <div class="widget">
                            <h3>Fast Links</h3>
                            <ul>
                                <li><a href="https://www.facebook.com">Facebook</a></li>
                                <li><a href="https://mail.google.com">Gmail</a></li>
                                <li><a href="https://twitter.com/">Twitter</a></li>
                                <li><a href="http://www.yahoo.com">Yahoo</a></li>
                                <li><a href="http://www.youtube.com">Youtube</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="span4">
                    <div class="padd">
                        <div class="widget">
                            <h3>Contact Me</h3>
                            <ul>
                                <li>Add: Philadelphia, PA 19104</li>
                                <li>Tel: 267-876-xxxx</li>
                                <li>Fax: 267-876-xxxx</li>
                                <li>Email: zc77@drexe.edu</li>
                                <li>Website: <a href="~/Home/">Computer Customized & Rating</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>


    <footer>
        <div class="container">
            <div class="row">
                <div class="span12">
                    <div class="fpadd">Copyright &copy; 2014 - <a href="~/Home/">Computer Customized & Rating</a></div>
                </div>
            </div>
            <div class="clearfix"></div>
        </div>
    </footer>

    <script type="text/javascript" src="~/Scripts/jquery-1.7.1.min.js"></script>
    @*<script src="//ajax.googleapis.com/ajax/libs/angularjs/1.3.0-rc.0/angular.min.js"></script>*@
    @*<script src="//ajax.googleapis.com/ajax/libs/angularjs/1.3.0-rc.0/angular-route.js"></script>*@
    <script type="text/javascript" src="~/Scripts/angular.min.js"></script>
    @*<script src="~/Scripts/angular.js"></script>*@
    @*<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.0.1/angular.min.js"></script>*@
    @*<script src="~/Scripts/angular-route.min.js"></script>*@
    <script type="text/javascript" src="~/Scripts/easing.js"></script>
    <script type="text/javascript" src="~/Scripts/bootstrap.js"></script>
    <script type="text/javascript" src="~/Scripts/ddlevelsmenu.js"></script>
    <script type="text/javascript" src="~/Scripts/flexslider.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery.prettyPhoto.js"></script>
    <script type="text/javascript" src="~/Scripts/custom.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery-ui-1.8.20.min.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
    <script type="text/javascript" src="~/Scripts/otf.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
